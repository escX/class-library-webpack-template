{
  "name": "js-library-webpack-template",
  "version": "1.0.0",
  "description": "A template of js library built with webpack",
  "main": "index.js",
  "scripts": {
    "dev": "webpack-dev-server --open --config webpack.dev.js",
    "build:ex": "webpack --config webpack.dev.js",
    "build": "webpack --config webpack.prod.js",
    "release": "npm run build:ex && npm run build",
    "gh": "git subtree push --prefix examples origin gh-pages"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/escX/js-library-webpack-template.git"
  },
  "keywords": [
    "js-library",
    "webpack-wemplate"
  ],
  "author": "escX",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/escX/js-library-webpack-template/issues"
  },
  "homepage": "https://github.com/escX/js-library-webpack-template#readme",
  "devDependencies": {
    "@babel/core": "^7.4.0",
    "@babel/preset-env": "^7.4.2",
    "babel-loader": "^8.0.5",
    "change-case": "^3.1.0",
    "clean-webpack-plugin": "^2.0.1",
    "html-webpack-plugin": "^3.2.0",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.3.0",
    "webpack-dev-server": "^3.2.1",
    "webpack-merge": "^4.2.1"
  }
}
